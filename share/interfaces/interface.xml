<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/org/skynet/asus/manager">
    <interface name="org.skynet.asus.Manager">
        <method name="getCurrentMode">
            <arg type="i" name="mode" direction="out" />
            <arg type="s" name="name" direction="out" />
        </method>
        <method name="setIntegrated">
            <arg type="i" name="result" direction="out" />
            <arg type="s" name="message" direction="out" />
        </method>
        <method name="setHybrid">
            <arg type="i" name="result" direction="out" />
            <arg type="s" name="message" direction="out" />
        </method>
        <method name="setXMobile">
            <arg type="i" name="result" direction="out" />
            <arg type="s" name="message" direction="out" />
        </method>
        <method name="setCompute">
            <arg type="i" name="result" direction="out" />
            <arg type="s" name="message" direction="out" />
        </method>
        <signal name="txStart">
            <arg type="s" name="id" />
            <arg type="s" name="info" />
        </signal>
        <signal name="txEnd">
            <arg type="s" name="id" />
            <arg type="i" name="code" />
            <arg type="s" name="status" />
        </signal>
    </interface>
</node>