<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/org/skynet/asus/manager">
    <interface name="org.skynet.asus.Manager">
        <method name="health">
            <arg type="i" name="code" direction="out" />
        </method>
        <method name="getCurrentActiveGPU">
            <arg type="i" name="index" direction="out" />
        </method>
        <method name="toggleActiveGPU">
        </method>
        <signal name="txStart">
            <arg type="s" name="id" />
            <arg type="s" name="info" />
        </signal>
        <signal name="txEnd">
            <arg type="s" name="id" />
            <arg type="i" name="code" />
        </signal>
    </interface>
</node>